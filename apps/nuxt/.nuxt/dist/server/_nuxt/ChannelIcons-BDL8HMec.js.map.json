\n\n// ËÆ°ÁÆóÂ≠¶‰π†ÁªüËÆ°Êï∞ÊçÆ\nconst studyStats = _computed(() => {\n  return {\n    total: practiceStore.total,\n    newWords: practiceStore.newWordNumber,\n    review: practiceStore.reviewWordNumber + practiceStore.writeWordNumber,\n    wrong: practiceStore.wrong,\n    correct: practiceStore.total - practiceStore.wrong,\n    time: msToHourMinute(practiceStore.spend),\n    date: dayjs().format('MMÊúàDDÊó•'),\n    dictionary: baseStore.sdict.name || 'Êú™Áü•ËØç‰π¶'\n  }\n})\n\n// Â§çÂà∂ÂõæÁâáÂà∞Ââ™Ë¥¥Êùø\nasync function copyImageToClipboard() {\n  try {\n    loading1.value = true\n    const snapdom = await loadJsLib('snapdom', LIB_JS_URL.SNAPDOM);\n    const blob = await snapdom.toBlob(posterEl.value, { scale: 2, type: 'png' })\n    if (!blob) throw new Error('capture failed')\n\n    if (navigator.clipboard && (window as any).ClipboardItem) {\n      await navigator.clipboard.write([new (window as any).ClipboardItem({ [blob.type || 'image/png']: blob })])\n      Toass.success('ÂõæÁâáÂ∑≤Â§çÂà∂Âà∞Ââ™Ë¥¥ÊùøÔºÅ')\n    } else {\n      await downloadImage()\n    }\n  } catch (error) {\n    Toass.error('Â§çÂà∂Â§±Ë¥•ÔºÅ')\n    await downloadImage()\n  } finally {\n    loading1.value = false\n  }\n}\n\n// ‰∏ãËΩΩÂõæÁâá\nasync function downloadImage() {\n  loading2.value = true\n  const snapdom = await loadJsLib('snapdom', LIB_JS_URL.SNAPDOM);\n  snapdom.download(posterEl.value, { scale: 2 })\n  loading2.value = false\n}\n\n// ÂàáÊç¢ËÉåÊôØ\nfunction changeBackground() {\n  const newIndex = Math.floor(Math.random() * 9) // 0-8\n  imgIndex.value = newIndex >= imgIndex.value ? newIndex + 1 : newIndex\n}\n\n// ËÆ°ÁÆóÂ≠¶‰π†ËøõÂ∫¶ÁôæÂàÜÊØî\nconst studyProgress = _computed(() => {\n  if (!baseStore.sdict.length) return 0\n  return Math.round((baseStore.sdict.lastLearnIndex / baseStore.sdict.length) * 100)\n})\n\nconst sentence = _computed(() => {\n  let list = [\n    { en: 'Actions speak louder than words.', cn: 'Ë°åÂä®ËÉú‰∫éË®ÄËØ≠' },\n    { en: 'Keep going, never give up!', cn: 'ÂùöÊåÅÂ∞±ÊòØËÉúÂà©' },\n    { en: 'Where there\\'s a will, there\\'s a way.', cn: 'ÊúâÂøóËÄÖ‰∫ãÁ´üÊàê' },\n    { en: 'Every cloud has a silver lining.', cn: 'ÈªëÊöó‰∏≠ÊÄªÊúâ‰∏ÄÁ∫øÂÖâÊòé' },\n    { en: 'Time heals all wounds.', cn: 'Êó∂Èó¥ËÉΩÊ≤ªÊÑà‰∏ÄÂàáÂàõ‰º§' },\n    { en: 'Never say die.', cn: 'Ê∞∏‰∏çË®ÄË¥•' },\n    { en: 'The best is yet to come.', cn: 'ÊúÄÂ•ΩÁöÑÂ∞öÊú™Âà∞Êù•' },\n    { en: 'Believe you can and you\\'re halfway there.', cn: 'Áõ∏‰ø°‰Ω†Ëá™Â∑±Ôºå‰Ω†Â∑≤ÁªèÊàêÂäü‰∫Ü‰∏ÄÂçä' },\n    { en: 'No pain, no gain.', cn: 'Ê≤°Êúâ‰ªòÂá∫Â∞±Ê≤°ÊúâÊî∂Ëé∑' },\n    { en: 'Dream big and dare to fail.', cn: 'Â§ßËÉÜÊ¢¶ÊÉ≥ÔºåÂãá‰∫éÂ§±Ë¥•' },\n    { en: 'Home is where the heart is.', cn: 'ÂøÉÂú®Âì™ÈáåÔºåÂÆ∂Â∞±Âú®Âì™Èáå' },\n    { en: 'Knowledge is power.', cn: 'Áü•ËØÜÂ∞±ÊòØÂäõÈáè' },\n    { en: 'Practice makes perfect.', cn: 'ÁÜüËÉΩÁîüÂ∑ß' },\n    { en: 'When in Rome, do as the Romans do.', cn: 'ÂÖ•‰π°Èöè‰øó' },\n    { en: 'Just do it.', cn: 'Âè™ÁÆ°ÂéªÂÅö' },\n    { en: 'So far, so good.', cn: 'Âà∞ÁõÆÂâç‰∏∫Ê≠¢Ôºå‰∏ÄÂàáËøòÂ•Ω' },\n    { en: 'The early bird catches the worm.', cn: 'Êó©Ëµ∑ÁöÑÈ∏üÂÑøÊúâËô´ÂêÉ' },\n    { en: 'Every day is a new beginning.', cn: 'ÊØè‰∏ÄÂ§©ÈÉΩÊòØÊñ∞ÁöÑÂºÄÂßã' },\n    { en: 'Success is a journey, not a destination.', cn: 'ÊàêÂäüÊòØÊóÖÁ®ãÔºå‰∏çÊòØÁªàÁÇπ' },\n    { en: 'Your only limit is your mind.', cn: '‰Ω†ÂîØ‰∏ÄÁöÑÈôêÂà∂ÊòØ‰Ω†ÁöÑÊÄùÁª¥' },\n    { en: 'A friend in need is a friend indeed.', cn: 'ÊÇ£ÈöæËßÅÁúüÊÉÖ' },\n    { en: 'Silence is golden.', cn: 'Ê≤âÈªòÊòØÈáë' },\n    { en: 'Let bygones be bygones.', cn: 'ËÆ©ËøáÂéªÁöÑÊàê‰∏∫ËøáÂéª' },\n    { en: 'Keep calm and carry on.', cn: '‰øùÊåÅÂÜ∑ÈùôÔºåÁªßÁª≠ÂâçËøõ' },\n    { en: 'Live and learn.', cn: 'Ê¥ªÂà∞ËÄÅÔºåÂ≠¶Âà∞ËÄÅ' },\n    { en: 'Mistakes are proof that you are trying.', cn: 'ÈîôËØØËØÅÊòé‰Ω†Âú®Âä™ÂäõÂ∞ùËØï' },\n    { en: 'Better late than never.', cn: 'ËøüÂÅöÊÄªÊØî‰∏çÂÅöÂ•Ω' },\n    { en: 'Be the change you wish to see in the world.', cn: 'Êàê‰∏∫‰Ω†Â∏åÊúõÂú®‰∏ñÁïå‰∏äÁúãÂà∞ÁöÑÊîπÂèò' },\n    { en: 'The journey of a thousand miles begins with a single step.', cn: 'ÂçÉÈáå‰πãË°åÔºåÂßã‰∫éË∂≥‰∏ã' },\n    { en: 'When one door closes, another opens.', cn: 'ÂΩì‰∏ÄÊâáÈó®ÂÖ≥Èó≠Êó∂ÔºåÂè¶‰∏ÄÊâá‰ºöÊâìÂºÄ' },\n  ]\n  return list[Math.floor(Math.random() * list.length)]\n})\n</script>\n\n<template>\n  <!-- ÂàÜ‰∫´Â≠¶‰π†ÊÄªÁªìÊåâÈíÆ -->\n  <BaseIcon @click=\"showShareDialog = true\"\n            class=\"bounce\">\n    <IconFluentShare20Regular class=\"text-blue-500 hover:text-blue-600\"/>\n  </BaseIcon>\n\n  <!-- Â≠¶‰π†ÊÄªÁªìÂàÜ‰∫´ÂõæÁâáÁîüÊàêÂØπËØùÊ°Ü -->\n  <Dialog v-model=\"showShareDialog\" title=\"ÂàÜ‰∫´\">\n    <div class=\"flex min-w-160 max-w-200 p-6 pt-0 gap-space\">\n      <!-- Â∑¶‰æßÔºöÊµ∑Êä•È¢ÑËßàÂå∫Âüü -->\n      <div ref=\"posterEl\" class=\"flex-1 border-r border-gray-200 bg-gray-100 rounded-xl  overflow-hidden relative\">\n        <div class=\"flex p-5 gap-space flex-col justify-between relative z-2 color-white h-full box-border\">\n          <div class=\"flex flex-col flex-1 space-y-3\">\n            <!-- È°∂ÈÉ®Áî®Êà∑‰ø°ÊÅØ -->\n            <div class=\"flex items-center\">\n              <div v-if=\"userStore.user?.username\"\n                   class=\"w-12 h-12 bg-gray-600 rounded-full mr-3 flex items-center justify-center\">\n                <IconSimpleIconsGithub class=\"w-6 h-6 text-white\"/>\n              </div>\n              <div>\n                <div class=\"font-semibold text-lg\">{{ userStore.user?.username }}</div>\n                <div class=\"\">{{ dayjs().format('YYYYÂπ¥MMÊúàDDÊó•') }}</div>\n              </div>\n              <div class=\"ml-auto text-xs\">\n                Type Words | Ëã±ËØ≠Â≠¶‰π†\n              </div>\n            </div>\n\n            <div class=\"bg-gray-900/30 py-4 center flex-col rounded-2xl\">\n              <div class=\"text-center mb-2 text-xl\">\n                ÊàëÂ≠¶‰π†‰∫Ü{{ studyStats.time }} {{ baseStore.sdict.name }}\n              </div>\n              <!-- Progress Overview -->\n              <div class=\"w-90/100 flex items-center gap-space\">\n                <div class=\"shrink-0\">ËøõÂ∫¶</div>\n                <Progress :percentage=\"studyProgress\" size=\"normal\"/>\n              </div>\n            </div>\n\n            <!-- ÁªüËÆ°Êï∞ÊçÆ -->\n            <div class=\"grid grid-cols-3 gap-4\">\n              <div class=\"stat-card\">\n                <div class=\"text-2xl font-bold\">{{ studyStats.newWords }}</div>\n                <div class=\"text-base\">Êñ∞ËØç</div>\n              </div>\n              <div class=\"stat-card\">\n                <div class=\"text-2xl font-bold\">{{ studyStats.review }}</div>\n                <div class=\"text-base\">Â§ç‰π†</div>\n              </div>\n              <div class=\"stat-card\">\n                <div class=\"text-2xl font-bold\">{{ studyStats.wrong }}</div>\n                <div class=\"text-base\">ÈîôËØç</div>\n              </div>\n            </div>\n\n            <!-- Âä±ÂøóËØ≠Âè• -->\n            <div class=\"bg-gray-900/30 py-4 rounded-2xl center flex-col flex-1 p-4\">\n              <div class=\"text-3xl text-center italic mb-2 en-article-family\">{{ sentence.en }}</div>\n              <div class=\"text-base italic\">{{ sentence.cn }}</div>\n            </div>\n          </div>\n\n          <!-- Â∫ïÈÉ®ÂìÅÁâå‰ø°ÊÅØ -->\n          <div class=\"bg-gray-900/30 py-4 rounded-2xl p-4\">\n            <div class=\"flex justify-between items-end\">\n              <div class=\"space-y-2\">\n                <div class=\"font-bold text-2xl\">Type Words</div>\n                <div class=\"text-base  \">{{ Origin }}</div>\n                <div class=\"text-xs  \">‰∏ÄÊ¨°Êï≤ÂáªÔºå‰∏ÄÁÇπËøõÊ≠•ÔºåÂºÄÊ∫êÂçïËØçÂ≠¶‰π†Â∑•ÂÖ∑</div>\n              </div>\n              <img :src=\"`/imgs/share/qr.png`\" class=\"w-20 w-20 rounded-md overflow-hidden\" alt=\"\" />\n            </div>\n          </div>\n        </div>\n\n        <img :src=\"`/imgs/share/bg/${imgIndex}.jpg`\" class=\"w-full object-cover object-center absolute top-0 \" alt=\"\" />\n      </div>\n\n      <!-- Âè≥‰æßÔºöÂàÜ‰∫´ÂºïÂØºÂå∫Âüü -->\n      <div class=\"flex-1 pt-0 \">\n        <div class=\"\">\n          <div class=\"text-2xl font-bold mb-4 flex items-center\">\n            <span class=\"mr-2\">üéØ</span>\n            ÂàÜ‰∫´‰Ω†ÁöÑËøõÊ≠•\n          </div>\n          <div class=\"flex items-start\">\n            <span class=\"mr-2\">üöÄ</span>\n            Âú® {{ APP_NAME }}ÔºåÂ≠¶‰π†Ëã±ËØ≠‰πüËÉΩÊàê‰∏∫Ë∂ÖÈÖ∑ÁöÑ‰∫ãÊÉÖÔºÅ\n          </div>\n          <div class=\"flex items-start\">\n            <span class=\"mr-2\">üì∏</span>\n            Âø´Êù•ÂàÜ‰∫´‰Ω†ÁöÑÂ≠¶‰π†ÂõæÁâáÔºåËÆ©‰Ω†ÁöÑËøõÊ≠•Âà∑Â±èÊúãÂèãÂúàÔºåÊàê‰∏∫ÊúÄÂèóÁû©ÁõÆÁöÑËã±ËØ≠Â≠¶Èú∏ÔºÅüòé\n          </div>\n          <div class=\"flex items-start\">\n            <span class=\"mr-2\">üí™</span>\n            Ëøô‰∏çÂè™ÊòØÁÆÄÂçïÁöÑÊâìÂç°ÔºåÊõ¥ÊòØ‰Ω†ÁßÄÂá∫Ëã±ËØ≠ÂÆûÂäõÁöÑËàûÂè∞ÔºÅ\n          </div>\n          <div class=\"flex items-start\">\n            <span class=\"mr-2\">üî•</span>\n            ÂàÜ‰∫´‰Ω†ÁöÑÂ≠¶‰π†ËÆ∞ÂΩïÔºåÊî∂Ëé∑ÊúãÂèã‰ª¨ÁöÑÁÇπËµûÂíåËÆ§ÂèØÔºåËÆ©‰Ω†ÁöÑÊúãÂèãÂúà‰πüÊéÄËµ∑‰∏ÄËÇ°Ëã±ËØ≠Â≠¶‰π†ÁöÑÁÉ≠ÊΩÆÔºÅ\n          </div>\n        </div>\n\n        <div class=\"space-y-4 mt-24\">\n          <!-- ‰∏™ÊÄßÂåñË£ÖÊâÆ -->\n          <div @click=\"changeBackground\"\n               class=\"flex items-center justify-start gap-space color-black px-6 py-3 bg-gray-200 rounded-lg cp  hover:bg-gray-300 transition-all duration-200\">\n            <IconMdiSparkles class=\"w-4 h-4 text-yellow-500\"/>\n            Êç¢‰∏™ËÉåÊôØ\n          </div>\n\n          <!-- ÂàÜ‰∫´ÊàòÁª© -->\n          <div @click=\"copyImageToClipboard\"\n               class=\"flex items-center justify-start gap-space px-6 py-3 bg-gradient-to-r from-green-500 to-green-600 text-white cp rounded-lg hover:from-green-600 hover:to-green-700 transition-all duration-200\">\n            <IconEosIconsLoading class=\"text-xl\" v-if=\"loading1\"/>\n            <IconFluentCopy20Regular class=\"w-5 h-5\" v-else/>\n            <span class=\"font-medium\">Â§çÂà∂Âà∞Ââ™Ë¥¥Êùø</span>\n          </div>\n\n          <div @click=\"downloadImage\"\n               class=\"flex items-center justify-start gap-space px-6 py-3 bg-gradient-to-r from-purple-500 to-purple-600 text-white cp rounded-lg hover:from-purple-600 hover:to-purple-700 transition-all duration-200\">\n            <IconEosIconsLoading class=\"text-xl\" v-if=\"loading2\"/>\n            <IconFluentArrowDownload20Regular class=\"w-5 h-5\" v-else/>\n            <span class=\"font-medium\">‰øùÂ≠òÈ´òÊ∏ÖÊµ∑Êä•</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </Dialog>\n</template>\n\n<style scoped lang=\"scss\">\n.stat-card {\n  @apply text-center bg-gray-900/30 py-4 rounded-2xl;\n}\n</style>\n","<script setup lang=\"ts\">\nimport { GITHUB } from '@/config/env.ts'\nimport BaseIcon from '@/components/BaseIcon.vue'\n</script>\n\n<template>\n  <a\n    :href=\"GITHUB\"\n    target=\"_blank\"\n    rel=\"noreferrer\"\n    :aria-label=\"$t('github_project')\"\n    class=\"color-reverse-black github\"\n  >\n    <BaseIcon title=\"Github\" v-bind=\"$attrs\">\n      <IconSimpleIconsGithub />\n    </BaseIcon>\n  </a>\n</template>\n\n<style scoped lang=\"scss\"></style>\n","<script setup lang=\"ts\">import { ref as _ref } from 'vue';\n\nimport BaseIcon from '@/components/BaseIcon.vue'\nimport { defineAsyncComponent } from 'vue'\nimport ShareIcon from '@/components/channel-icons/ShareIcon.vue'\nimport WeChat from '@/components/channel-icons/WeChat.vue'\nimport Github from '@/components/channel-icons/Github.vue'\n\ndefineProps({\n  type: { default: 'vertical' },\n  share: { type: Boolean, default: true },\n  wechat: { type: Boolean, default: true },\n  github: { type: Boolean, default: true }\n})\nconst Dialog = defineAsyncComponent(() => import('@/components/dialog/Dialog.vue'))\n\nlet showXhsDialog = _ref(false)\nlet showQQDialog = _ref(false)\n</script>\n\n<template>\n  <div class=\"center\" :class=\"type === 'vertical' ? 'flex-col gap-1' : 'gap-4'\">\n    <Github v-if=\"github\" />\n\n    <WeChat v-if=\"wechat\" />\n\n    <BaseIcon :title=\"$t('qq_group')\" @click=\"showQQDialog = true\">\n      <IconUiwQq class=\"color-red\" />\n    </BaseIcon>\n    <BaseIcon :title=\"$t('xiaohongshu')\" @click=\"showXhsDialog = true\">\n      <IconSimpleIconsXiaohongshu class=\"color-red-500\" />\n    </BaseIcon>\n\n    <a\n      href=\"https://x.com/typewords2\"\n      target=\"_blank\"\n      rel=\"noreferrer\"\n      :aria-label=\"$t('follow_x_account', { x_account: 'typewords2' })\"\n    >\n      <BaseIcon :title=\"$t('twitter')\">\n        <IconRiTwitterFill class=\"color-blue\" />\n      </BaseIcon>\n    </a>\n\n    <a\n      href=\"mailto:zyronon@163.com\"\n      target=\"_blank\"\n      rel=\"noreferrer\"\n      :aria-label=\"$t('send_email', { email: 'zyronon@163.com' })\"\n    >\n      <BaseIcon :title=\"$t('email')\">\n        <IconMaterialSymbolsMail class=\"color-blue\" />\n      </BaseIcon>\n    </a>\n\n    <ShareIcon v-if=\"share\" />\n  </div>\n\n  <Dialog v-model=\"showXhsDialog\" :title=\"$t('xiaohongshu')\">\n    <div class=\"w-120 p-6 pt-0\">\n      <div class=\"mb-4\">\n        {{ $t('xiaohongshu_desc') }}\n      </div>\n      <div class=\"text-center\">\n        <img src=\"/imgs/channel/xhs.png\" :alt=\"$t('xiaohongshu_qrcode')\" class=\"w-70 rounded-xl shadow-lg\" />\n      </div>\n    </div>\n  </Dialog>\n\n  <Dialog v-model=\"showQQDialog\" :title=\"$t('qq_group')\">\n    <div class=\"w-120 p-6 pt-0\">\n      <div class=\"mb-4\">\n        <span>{{ $t('community_desc') }}</span>\n      </div>\n      <div class=\"text-center\">\n        <img src=\"/imgs/channel/qq.jpg\" :alt=\"$t('qq_qrcode')\" class=\"w-70 rounded-xl shadow-lg\" />\n      </div>\n    </div>\n  </Dialog>\n</template>\n<style scoped lang=\"scss\"></style>\n"],"version":3}